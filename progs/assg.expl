decl
   int sum,t,i,j,k,n;
   int a[100];  
enddecl

int main()
{
   sum = 0;
   read(n);
   i = 1;
   while(i < n) do
	j = 1;
	while(j < n) do
	    k = 1;
	    while(k < n) do
		read(t);
		if(t == 0) then
		    break;
		endif;
		sum = sum + t;
		k = k + 1;
	    endwhile; 
	    j = j + 1;
	endwhile;
	i = i + 1;
   endwhile;
   write(sum);
   return 0;
}
